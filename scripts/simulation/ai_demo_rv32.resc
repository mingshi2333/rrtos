:name: ai_demo_virt_rv32
:description: RISC-V 32-bit virt machine for ai_demo

# Use path without @ to avoid relative path confusion
$bin?="build/apps/ai_demo/ai_demo"

mach create "riscv32-virt"

machine LoadPlatformDescription @scripts/simulation/riscv32_virt.repl

showAnalyzer sysbus.uart0

sysbus LoadELF $bin
start
